	option down '3'
	option up '3'

config policy 'balanced'
	list use_member 'member_1'
	list use_member 'member_2'
	list use_member 'member_3'
	option last_resort 'default'

config rule 'default_rule'
	option dest_ip '0.0.0.0/0'
	option use_policy 'balanced'
	option proto 'all'
	option sticky '0'

config member 'member_1'
	option interface 'wan_3g_1'
	option metric '1'
	option weight '3'

config member 'member_2'
	option interface 'wan_3g_2'
	option metric '1'
	option weight '2'

